{
  "name": "@cycle/history",
  "version": "4.0.0",
  "description": "The standard history driver for Cycle.js",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "directories": {
    "test": "test"
  },
  "repository": "https://github.com/cyclejs/cyclejs/tree/master/packages/history",
  "author": "Tylor Steinberger",
  "license": "MIT",
  "bugs": "https://github.com/cyclejs/cyclejs/issues",
  "homepage": "https://cycle.js.org",
  "dependencies": {
    "history": "^3.0.0"
  },
  "devDependencies": {
    "@cycle/base": "^4.1.1",
    "@cycle/dom": "^12.2.4",
    "@cycle/most-adapter": "^4.0.1",
    "@cycle/most-run": "^4.1.0",
    "@cycle/rxjs-adapter": "^3.0.2",
    "@cycle/rxjs-run": "^3.0.3",
    "@cycle/xstream-adapter": "^3.0.3",
    "@cycle/xstream-run": "^3.1.0",
    "assert": "^1.4.1",
    "most": "^1.0.1",
    "rx": "^4.1.0",
    "rxjs": "^5.0.0-beta.11",
    "saucie": "^1.4.1",
    "xstream": "^6.2.0"
  },
  "scripts": {
    "lint": "../node_modules/.bin/tslint -c tslint.json src/*.ts src/**/*.ts",
    "test-node": "../node_modules/.bin/mocha -r babel-register test/node/*.js",
    "build-browser-tests": "../node_modules/.bin/browserify -t babelify test/browser/index.js -o test/browser/bundle.js",
    "test-browser": "../node_modules/.bin/testem ci -l Chrome,Firefox",
    "test-browser-ci": "../node_modules/.bin/testem ci",
    "test": "npm run lint && npm run lib && npm run test-node && npm run test-browser",
    "test-ci": "npm run lint && npm run lib && npm run test-node && npm run test-browser-ci",
    "predist": "rm -rf dist/ && mkdir -p dist/",
    "prelib": "../node_modules/.bin/typings install",
    "lib": "../node_modules/.bin/tsc",
    "start": "npm install && npm prune",
    "prerelease": "npm run lib",
    "release": "npm run release-patch",
    "release-patch": "git checkout master && release patch && npm publish --access=public",
    "release-minor": "git checkout master && release minor && npm publish --access=public",
    "release-major": "git checkout master && release major && npm publish --access=public"
  }
}
